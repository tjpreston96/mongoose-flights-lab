<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <title>Flights</title>
</head>
<body>
  <h1>All Flights</h1>
  <table>
    <thead>
      <tr>
        <th>Airport</th>
        <th>Airline</th>
        <th>Flight No.</th>
        <th>Depart</th>
        <!-- <th>Departed</th> -->
      </tr>
    </thead>
    <tbody>
      <!-- Write the line of EJS to iterate over movies using forEach -->
      <% flights.forEach(function(f) { %>
        <tr>
          <td><%= f.airport %></td>
          <td><%= f.airline %></td>
          <td><%= f.flightNo %></td>
          <td><%= f.departs %></td>
        </tr>
        
          <% }) %> 
    </tbody>
  </table>
</body>
</html>